{
    "name": "websublime/pressly",
    "version": "0.3",
    "description": "A library for Wordpress themes.",
    "type": "library",
    "keywords": ["library", "websublime", "pressly", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Ramos",
            "email": "miguel.marques.ramos@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.14",
        "pimple/pimple": "1.*",
        "symfony/config": "2.1.0",
        "symfony/http-kernel": "2.1.0",
        "symfony/http-foundation": "2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "3.7.1",
        "mockery/mockery": "0.7.*"
    },
    "autoload": {
        "psr-0": {
            "Wordpress\\Foundation": "src/"
        },
        "files": [
            "src/underscore.php",
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev"
}
